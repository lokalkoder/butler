{
  "name": "lokal/butler",
  "description": "Lokal Koder Application Butler",
  "license": "MIT",
  "authors": [
    {
      "name": "joesama",
      "email": "joharijumali@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.31",
    "illuminate/database": "^11.31",
    "illuminate/http": "^11.31",
    "illuminate/collections": "^11.31",
    "league/fractal": "^0.20.1",
    "prettus/l5-repository": "^2.9",
    "spatie/laravel-enum": "^3.1",
    "spatie/php-structure-discoverer": "^2.2",
    "spatie/laravel-activitylog": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "Lokal\\Butler\\": "src/"
    },
    "classmap": [],
    "files": [
      "helpers/support.php",
      "helpers/directory.php",
      "helpers/response.php",
      "helpers/translation.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lokal\\Butler\\Provider"
      ]
    }
  },
  "require-dev": {
    "pestphp/pest": "^3.7",
    "mockery/mockery": "^1.6",
    "fakerphp/faker": "^1.23",
    "orchestra/testbench": "^9.5"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
